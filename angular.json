{
  "$schema": "https://raw.githubusercontent.com/angular/cli/main/src/command-line/schemas/json-schema.json",
  "version": 1,
  "defaultProject": "stockManager",
  "projects": {
    "stockManager": {
      "projectType": "application",
      "root": "",
      "sourceRoot": "src",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:browser",
          "options": {
            "outputPath": "dist/stockManager",
            "index": "src/index.html",
            "main": "src/main.ts",
            "polyfills": "src/polyfills.ts",
            "tsConfig": "tsconfig.app.json",
            "assets": [
              "src/favicon.ico",
              "src/assets"
            ],
            "styles": [
              "src/styles.scss"
            ],
            "scripts": []
          }
        },
        "serve": {
          "builder": "@angular-devkit/build-angular:dev-server",
          "options": {
            "buildTarget": "stockManager:build"
          }
        }
      }
    }
  },
  "cli": {
    "analytics": false
  }
}
